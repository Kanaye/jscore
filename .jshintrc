{
  "eqeqeq": false,
  "forin": false,
  "plusplus": false,

  "es3": true,
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "freeze": true,
  "immed": true,
  "indent": 2,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonbsp": true,
  "nonew": true,
  "quotmark": "single",
  "undef": true,
  "unused": "vars",
  "trailing": true,
  "eqnull": true,

  "browser": true,

  "globals": {
    "define": true,
    "module": true
  }
}
